{
  "manifest_version": 3,
  "name": "Website Scanner",
  "version": "1.0",
  "icons": {
    "48": "assets/malware48.png",
    "128": "assets/malware128.png"
  },
  "action": {
    "default_icon": {
    
    "48": "assets/malware48.png",
    "128": "assets/malware128.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "scripting", "tabs"],
  "host_permissions": ["<all_urls>"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
            // test
        }
    ]
}
